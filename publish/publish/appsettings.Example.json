{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_server;Port=3306;Database=your_database;Uid=your_username;Pwd=your_password;"
  },
  "Jwt": {
    "Key": "your_jwt_secret_key_minimum_32_chars_long",
    "Issuer": "https://your-issuer-url",
    "Audience": "https://your-audience-url",
    "ExpiryInMinutes": 60
  }
}
